{

  "manifest_version": 2,

  "name": "YouTube-Proxy",
  "description": "Run an HTTPS proxy server on youtube.com",
  "version": "2.0",
  "background": {
  "scripts":["popup.js"]
  },
  "icons": { "16": "icon.png",
             "48": "icon48.png",
            "128": "icon128.png" },
  "permissions": [
        "proxy"
    ],
    "browser_action": {
      "default_title": "Choose a proxy server.",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  }

}

