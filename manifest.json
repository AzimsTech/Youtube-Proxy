{

  "manifest_version": 2,

  "name": "YouTube-Proxy",
  "description": "Run an HTTPS proxy server on youtube.com",
  "version": "1.0",
  "background": {
  "scripts":["proxy.js"]
  },
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" },
  "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*",
        "notifications",
        "contextMenus",
        "history",
        "background",
        "proxy"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}